  "scripts": {
    "dev": "node node_modules/.store/vite@5.4.21/node_modules/vite/bin/vite.js",
    "build": "node node_modules/.store/vite@5.4.21/node_modules/vite/bin/vite.js build && electron-builder",
    "preview": "node node_modules/.store/vite@5.4.21/node_modules/vite/bin/vite.js preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
