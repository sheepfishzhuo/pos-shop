    "dayjs": "^1.11.10",
    "recharts": "^2.10.3",
    "antd": "^5.12.2",
    "@ant-design/icons": "^5.2.6",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@esbuild/win32-x64": "^0.21.5",
    "@rollup/rollup-win32-x64-msvc": "^4.59.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
